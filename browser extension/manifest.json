{
  "manifest_version": 3,
  "name": "crateplug",
  "version": "1.0",
  "permissions": ["tabs"],
  "host_permissions": ["http://127.0.0.1:48721/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "download",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
